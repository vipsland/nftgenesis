{
  "extends": "solhint:recommended",
  "plugins": [
    "prettier"
  ],
  "rules": {
    "avoid-throw": "off",
    "avoid-suicide": "error",
    "avoid-sha3": "warn",
    "var-name-mixedcase": "off",
    "func-name-mixedcase": "off",
    "compiler-version": [
      "error",
      "^0.8.17"
    ]
  }
}